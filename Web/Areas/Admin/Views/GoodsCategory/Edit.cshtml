

@using EFModel;
@model GoodsCategory
<form id="edit_form">
    <input name="ID" value="@Model.ID" type="hidden" />
    <input name="dInsertTime" value="@Model.dInsertTime" type="hidden" />
<table class="gridtable">
    <tr>
        <td>图片分类名称</td>
        <td>
            <input class="easyui-textbox" name="GoodsCatetoryName" value="@Model.GoodsCatetoryName" />
        </td>
    </tr>
    <tr>
        <td>分类图片上传</td>
        <td>
            <div id="sPath"></div>
            <input type="hidden" name="sPath" value="@Model.sPath" />
        </td>
    </tr>
</table>
</form>
